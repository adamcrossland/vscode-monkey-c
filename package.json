{
    "name": "monkey-c",
    "displayName": "Monkey C",
    "description": "Garmin Connect IQ Monkey C language support for VS Code",
    "icon": "images/icon.svg",
    "galleryBanner": {
        "color": "#424242",
        "theme": "dark"
    },
    "version": "0.3.1",
    "publisher": "ghisguth",
    "engines": {
        "vscode": "^1.5.0"
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/ghisguth/vscode-monkey-c/issues",
        "email": "alexander.fedora@gmail.com"
    },
    "homepage": "https://github.com/ghisguth/vscode-monkey-c/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/ghisguth/vscode-monkey-c.git"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onCommand:extension.newiqproject"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "monkeyc",
                "aliases": [
                    "Monkey C",
                    "monkeyc",
                    "garmin",
                    "Connect IQ",
                    "connect iq",
                    "connectiq"
                ],
                "extensions": [
                    ".mc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "monkeyc",
                "scopeName": "source.mc",
                "path": "./syntaxes/monkeyc.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "monkeyc",
                "path": "./snippets/monkeyc.json"
            }
        ],
        "commands": [
            {
                "command": "extension.newiqproject",
                "title": "New IQ Project"
            }
        ]
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32",
        "web-request": "^1.0.7"
    }
}